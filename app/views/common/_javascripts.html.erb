<script>
  function initMap() {
    $(document).trigger('googlemaps:loaded');
  }
</script>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js"></script>
<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{Rails.application.secrets.google_maps_api_key}&callback=initMap", async: true, defer: true, 'data-turbolinks-eval': false %>
<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>


